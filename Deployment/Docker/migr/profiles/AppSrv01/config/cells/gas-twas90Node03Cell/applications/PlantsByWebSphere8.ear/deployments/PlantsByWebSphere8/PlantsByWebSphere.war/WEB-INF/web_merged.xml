<?xml version="1.0" encoding="UTF-8"?><web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>PlantsByWebSphere</display-name>
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <context-param>
        <param-name>javax.faces.VALIDATE_EMPTY_FIELDS</param-name>
        <param-value>false</param-value>
    </context-param>
    <servlet>
        <servlet-name>FacesServlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>com.ibm.websphere.samples.pbw.war.AccountServlet</servlet-name>
        <servlet-class>com.ibm.websphere.samples.pbw.war.AccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>com.ibm.websphere.samples.pbw.war.ImageServlet</servlet-name>
        <servlet-class>com.ibm.websphere.samples.pbw.war.ImageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>com.ibm.websphere.samples.pbw.war.AdminServlet</servlet-name>
        <servlet-class>com.ibm.websphere.samples.pbw.war.AdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FacesServlet</servlet-name>
        <url-pattern>*.jsf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.ibm.websphere.samples.pbw.war.AccountServlet</servlet-name>
        <url-pattern>/servlet/AccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.ibm.websphere.samples.pbw.war.ImageServlet</servlet-name>
        <url-pattern>/servlet/ImageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.ibm.websphere.samples.pbw.war.AdminServlet</servlet-name>
        <url-pattern>/servlet/AdminServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>10</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <error-page>
        <exception-type>javax.faces.application.ViewExpiredException</exception-type>
        <location>/viewExpired.xhtml</location>
    </error-page>
    <error-page>
        <location>/error.jsp</location>
    </error-page>
    <security-constraint>
        <web-resource-collection>
            <web-resource-name>SampAdmin</web-resource-name>
            <description>Sample Admin</description>
            <url-pattern>/adminactions.html</url-pattern>
            <url-pattern>/adminbanner.html</url-pattern>
            <url-pattern>/backorderadmin.jsp</url-pattern>
            <url-pattern>/servlet/AdminServlet</url-pattern>
            <url-pattern>/supplierconfig.jsp</url-pattern>
            <http-method>GET</http-method>
            <http-method>PUT</http-method>
            <http-method>HEAD</http-method>
            <http-method>TRACE</http-method>
            <http-method>POST</http-method>
            <http-method>DELETE</http-method>
            <http-method>OPTIONS</http-method>
        </web-resource-collection>
        <auth-constraint>
            <description>Samples Administrator</description>
            <role-name>SampAdmin</role-name>
        </auth-constraint>
        <user-data-constraint>
            <transport-guarantee>NONE</transport-guarantee>
        </user-data-constraint>
    </security-constraint>
    <login-config>
        <auth-method>BASIC</auth-method>
        <realm-name>Default</realm-name>
    </login-config>
    <security-role>
        <description>Samples Administrator</description>
        <role-name>SampAdmin</role-name>
    </security-role>
    <ejb-local-ref>
        <ejb-ref-name>com.ibm.websphere.samples.pbw.war.AccountServlet/catalog</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.ibm.websphere.samples.pbw.ejb.CatalogMgr</local>
        <injection-target>
            <injection-target-class>com.ibm.websphere.samples.pbw.war.AccountServlet</injection-target-class>
            <injection-target-name>catalog</injection-target-name>
        </injection-target>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>com.ibm.websphere.samples.pbw.war.AccountServlet/login</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.ibm.websphere.samples.pbw.ejb.CustomerMgr</local>
        <injection-target>
            <injection-target-class>com.ibm.websphere.samples.pbw.war.AccountServlet</injection-target-class>
            <injection-target-name>login</injection-target-name>
        </injection-target>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>com.ibm.websphere.samples.pbw.war.AdminServlet/catalog</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.ibm.websphere.samples.pbw.ejb.CatalogMgr</local>
        <injection-target>
            <injection-target-class>com.ibm.websphere.samples.pbw.war.AdminServlet</injection-target-class>
            <injection-target-name>catalog</injection-target-name>
        </injection-target>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>com.ibm.websphere.samples.pbw.war.AdminServlet/suppliers</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.ibm.websphere.samples.pbw.ejb.SuppliersBean</local>
        <injection-target>
            <injection-target-class>com.ibm.websphere.samples.pbw.war.AdminServlet</injection-target-class>
            <injection-target-name>suppliers</injection-target-name>
        </injection-target>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>com.ibm.websphere.samples.pbw.war.AdminServlet/login</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.ibm.websphere.samples.pbw.ejb.CustomerMgr</local>
        <injection-target>
            <injection-target-class>com.ibm.websphere.samples.pbw.war.AdminServlet</injection-target-class>
            <injection-target-name>login</injection-target-name>
        </injection-target>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>resetDB</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.ibm.websphere.samples.pbw.ejb.ResetDBBean</local>
        <injection-target>
            <injection-target-class>com.ibm.websphere.samples.pbw.war.AdminServlet</injection-target-class>
            <injection-target-name>resetDB</injection-target-name>
        </injection-target>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>com.ibm.websphere.samples.pbw.war.AdminServlet/backOrderStock</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.ibm.websphere.samples.pbw.ejb.BackOrderMgr</local>
        <injection-target>
            <injection-target-class>com.ibm.websphere.samples.pbw.war.AdminServlet</injection-target-class>
            <injection-target-name>backOrderStock</injection-target-name>
        </injection-target>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>com.ibm.websphere.samples.pbw.war.ImageServlet/catalog</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.ibm.websphere.samples.pbw.ejb.CatalogMgr</local>
        <injection-target>
            <injection-target-class>com.ibm.websphere.samples.pbw.war.ImageServlet</injection-target-class>
            <injection-target-name>catalog</injection-target-name>
        </injection-target>
    </ejb-local-ref>
</web-app>